@model IEnumerable<Eproject3.Models.Users>
@using Eproject3.Repo
@{
    Repository r = new Repository();
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UPass)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UAdress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Img)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Exp_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Packs.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Roles.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.AccNum)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.UPhone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UPass)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.UAdress)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Img)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Exp_Date)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Packs.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Roles.name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.AccNum)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.id }) |
        @Html.ActionLink("Details", "Details", new { id = item.id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.id })
    </td>
</tr>
}

</table>

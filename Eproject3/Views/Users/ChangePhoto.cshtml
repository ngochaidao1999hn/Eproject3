@using Eproject3.Models
@{
    ViewBag.Title = "ChangePhoto";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var isvalid = (Users)Session["user"];
}


@ViewBag.FileStatus
<div class="col-sm-12">
    <div class="col-sm-3"></div>
    <div class="col-sm-5 form-style">
        <h2>ChangePhoto</h2>
        <form method="post" enctype="multipart/form-data" action="/Users/ChangePhoto">
            Old Photo:<img style="width:200px;height:160px" src="/images/@isvalid.Img" />
            <input type="file" name="url" /><br />
            <button class="btn btn-default">Change</button>
        </form>
    </div>

</div>
<br />